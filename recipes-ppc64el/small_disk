partman-auto/text/small_disk ::

8 1 1 prep
	$primary{ }
	$bootable{ }
	method{ prep } .

768 788 1024 ext2
	$primary{ }
	method{ format }
	format{ }
	use_filesystem{ }
	filesystem{ ext2 }
	mountpoint{ /boot } .

2000 2840 -1 $default_filesystem
	$lvmok{ }
	$primary{ }
	method{ format }
	format{ }
	use_filesystem{ }
	$default_filesystem{ }
	mountpoint{ / } .

500 560 100% linux-swap
	$lvmok{ }
	method{ swap }
	format{ } .
